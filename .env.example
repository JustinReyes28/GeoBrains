# Environment Variables Template
# Copy this file to .env and fill in your values

# Prisma Database URL for PostgreSQL
# Example for local development: postgresql://USER:PASSWORD@localhost:5432/geobrains
POSTGRES_PRISMA_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Non-pooling connection URL for direct database access
# Example for local development: postgresql://USER:PASSWORD@localhost:5432/geobrains
POSTGRES_URL_NON_POOLING="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Vercel Deployment (Optional but recommended for GeoBrains)
# POSTGRES_PRISMA_URL=""
# POSTGRES_URL_NON_POOLING=""
